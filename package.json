{
  "name": "iitk-general-elections",
  "version": "1.0.0",
  "description": "Voting system for IITK General Elections",
  "main": "index.html",
  "scripts": {
    "post_install": "bower install",
    "test": "nwbuild -r . -v 0.11.5",
    "build": "rm -rf ./build && nwbuild -p 'win64' -v 0.11.5 -o build ."
  },
  "keywords": [
    "IITK",
    "elections",
    "voting"
  ],
  "window": {
    "toolbar": false,
    "width": 1366,
    "height": 968,
    "min_width": 1024,
    "min_height": 600,
    "max_width": 1366,
    "max_height": 968,
    "position": "mouse",
    "fullscreen": true
  },
  "webkit": {
    "plugin": false,
    "java": false,
    "page-cache": false
  },
  "author": "Srijan R Shetty <srijan.shetty+code@gmail.com> (http://srijanshetty.in/)",
  "license": "MIT",
  "devDependencies": {
    "node-webkit-builder": "^1.0.7"
  }
}
