{
  "name": "iitk-general-elections",
  "version": "2018.1.0",
  "description": "Voting system for IITK General Elections",
  "main": "index.html",
  "scripts": {
    "post_install": "bower install",
    "build": "build --tasks win-x64,linux-x64,mac-x64 --mirror https://dl.nwjs.io/ .",
    "start": "run --x64 --mirror https://dl.nwjs.io/ ."
  },
  "build": {
    "nwVersion": "0.27.4",
    "targets": ["zip"]
  },
  "keywords": [
    "IITK",
    "elections",
    "voting"
  ],
  "window": {
    "toolbar": false,
    "min_width": 1024,
    "min_height": 600,
    "position": "mouse",
    "fullscreen": true
  },
  "author": "Srijan R Shetty <srijan.shetty+code@gmail.com> (http://srijanshetty.in/)",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.8.2",
    "nwjs-builder-phoenix": "^1.14.6"
  }
}
